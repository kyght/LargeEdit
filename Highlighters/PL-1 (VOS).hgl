        <KeyList Name="PL/1 Compiler functions" Enabled="True" Style="" Words="%include&#xA;%list&#xA;%nolist&#xA;%options&#xA;%page&#xA;%replace&#xA;">
          <Attributes Foreground="clBlue" Background="clWhite" Style=""/>
