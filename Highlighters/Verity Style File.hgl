    <MainRules Name="Root" Enabled="True" Style="">
      <Delimiters Value="!&quot;%&amp;'()*+,./;&lt;=&gt;?@[\]^{|}~"/>
      <Attributes Foreground="clBlack" Background="clWhite" Style=""/>
      <Properties/>
      <OpenToken/>
      <CloseToken/>
      <MultiTokens>
        <Tokens/>
      </MultiTokens>
      <SubRules>
        <KeyList Name="Key Words 1" Enabled="True" Style="" Words="$$&#xA;$control:&#xA;$define&#xA;$endif&#xA;$include&#xA;$ifdef&#xA;$subst:&#xA;">
          <Attributes Foreground="clGreen" Background="clWhite" Style=""/>
        </KeyList>
        <KeyList Name="Key Words 2" Enabled="True" Style="" Words="BulkLoad&#xA;default&#xA;false&#xA;FastSearch&#xA;guess&#xA;NewsFeedIdx&#xA;NewsFeedOpt&#xA;no&#xA;none&#xA;ReadOnly&#xA;skip&#xA;true&#xA;yes&#xA;8859&#xA;">
          <Attributes Foreground="clBlue" Background="clWhite" Style=""/>
        </KeyList>
        <KeyList Name="Key Words 3" Enabled="True" Style="" Words="agents:&#xA;agt:&#xA;autorec:&#xA;autoval:&#xA;collection:&#xA;config:&#xA;constant:&#xA;copy:&#xA;data-table:&#xA;dda:&#xA;default:&#xA;define:&#xA;descriptor:&#xA;dft:&#xA;dlv:&#xA;drivers:&#xA;dispatch:&#xA;field:&#xA;fixwidth:&#xA;gateway:&#xA;lex:&#xA;mode:&#xA;policy:&#xA;repository:&#xA;session:&#xA;table:&#xA;token:&#xA;type:&#xA;types:&#xA;user:&#xA;varwidth:&#xA;vdkwatch:&#xA;worm:&#xA;zone-begin:&#xA;zone-end:&#xA;">
          <Attributes Foreground="$00FF0080" Background="clWhite" Style=""/>
        </KeyList>
        <KeyList Name="Key Words 4" Enabled="True" Style="" Words="date&#xA;EOS&#xA;multiRow&#xA;multiRowOrderBy&#xA;multiRowCol&#xA;multiRowFormat&#xA;NEWLINE&#xA;PARA&#xA;PUNCT&#xA;signed-integer&#xA;sirepath&#xA;TAB&#xA;text&#xA;unsigned-integer&#xA;WHITE&#xA;WORD&#xA;">
          <Attributes Foreground="$000080FF" Background="clWhite" Style=""/>
