        <KeyList Name="Templates" Enabled="True" Style="" Words="Template&#xA;">
          <Attributes Foreground="clBlue" Background="clWhite" Style=""/>
        </KeyList>
        <KeyList Name="Nesting Statements" Enabled="True" Style="" Words="Else&#xA;EndIf&#xA;EndLoop&#xA;EndSelect&#xA;ExitLoop&#xA;IfCharIs&#xA;IfColNum&#xA;IfEof&#xA;IfFound&#xA;IfNotFound&#xA;IfSel&#xA;Loop&#xA;StartSelect&#xA;">
          <Attributes Foreground="$00FF0080" Background="clWhite" Style=""/>
        </KeyList>
        <KeyList Name="Key Commands" Enabled="True" Style="" Words="All&#xA;ARROW&#xA;AllFiles&#xA;Backspace&#xA;DEL&#xA;DOWN&#xA;END&#xA;HOME&#xA;IgnoreCase&#xA;Key&#xA;LEFT&#xA;PGDN&#xA;PGUP&#xA;RemoveDup&#xA;RIGHT&#xA;Selected&#xA;Text&#xA;UP&#xA;">
